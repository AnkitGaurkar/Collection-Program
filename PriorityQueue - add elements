/*Queue:- is maintain of First In First Order(FIFO)...
 * 
 * ProrityQueue:- PQ implements to the Queue interface...
 * 
 * Its hold their Object or elements which  process in their Priorities..
 * ProrityQueue doest not allow null value in Queue..
 */
package com;

import java.util.Iterator;
import java.util.PriorityQueue;

class Demo2
{
	public static void main(String[]args)
	{
		PriorityQueue <String> pq= new PriorityQueue <String> ();
		
		pq.add("Ankit");
		pq.add("Ankit");
		pq.add("Rohan");
		
		System.out.println(pq.element());
		System.out.println(pq.peek());
		
		System.out.println("After iterating");
		Iterator<String> itr=pq.iterator();
					while(itr.hasNext())
					{
						System.out.println(itr.next());
					}
					pq.remove();
					pq.poll();
					
					System.out.println("After remove 2 elements");
					Iterator<String> itr2=pq.iterator();
					while(itr2.hasNext())
					{
						System.out.println(itr2.next());
					}
	}		
	
}
