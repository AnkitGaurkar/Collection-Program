package com;

import java.util.Arrays;
import java.util.LinkedList;

public class Demo2 {

	
	public static void main(String[] args) {
	
		String s[]=new String[] {"Ankit","Sunil","Gaurkar"};
		
		System.out.println("Print String array:-"+ Arrays.toString(s));
		
		System.out.println("To Convert Array into LinkedList:-");
		LinkedList<String>l1=new LinkedList<String>();
		
		for(String a:s)
		{
			l1.add(a);
		}
		System.out.println(l1);
		
		System.out.println("Print Integer no:-");
		
		int arr[]=new int[] {1,3,4,5,6,5};
		
		System.out.println(Arrays.toString(arr));
	}
	
	
}

