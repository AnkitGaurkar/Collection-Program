package com;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collection;
import java.util.Collections;
import java.util.List;

public class Demo2 {

	
	public static void main(String[] args) {
		
		ArrayList<String>a1=new ArrayList<String>();
		a1.add("Ankit");
		a1.add("Sunil");
		a1.add("Gaurkar");
		a1.add("Sangita");
		a1.add("Rohan");
		
		String Family[]=a1.toArray(new String[a1.size()]);
		
		System.out.println("After Converting ArrayList into Array:-");
		System.out.println(Family.length);
		
		
		for(String f:Family)
		{
			System.out.println(f);
		}
		
		System.out.println();
		
		System.out.println("To Convert Array to ArrayList:-");
		
		List<String>a2=new ArrayList<String>();
		a2= Arrays.asList(Family);
		
		System.out.println(a2);
				
		
	}
	
	}

